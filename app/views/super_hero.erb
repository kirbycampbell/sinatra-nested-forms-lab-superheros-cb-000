    <li><label for="member2_name">Member 2 Name:</label><input type="text" id="member2_name" name="member2[name]"></input></li>
    <li><label for="member2_power">Member 2 Power:</label><input type="text" id="member2_power" name="member2[power]"></input></li>
    <li><label for="member2_bio">Member 2 Bio:</label><input type="text" id="member2_bio" name="member2[bio]"></input></li>
    <li><label for="member3_name">Member 3 Name:</label><input type="text" id="member3_name" name="member3[name]"></input></li>
    <li><label for="member3_power">Member 3 Power:</label><input type="text" id="member3_power" name="member3[power]"></input></li>
    <li><label for="member3_bio">Member 3 Bio:</label><input type="text" id="member3_bio" name="member3[bio]"></input></li>
    <li><input type="submit" value="Submit">
  </ul>
  </form>

</body>
</html>
